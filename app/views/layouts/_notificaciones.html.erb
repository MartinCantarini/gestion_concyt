<div class="row">

  <% if notice %>
    <div class="col-md-12">
      <p class="alert alert-success" style="font-size:1.0625em;"><strong><span class="glyphicon glyphicon-exclamation-sign"></span> <%= notice %></strong></p>
    </div>
  <% end %>
  <% if alert %>
    <div class="col-md-12">
      <p class="alert alert-danger" style="font-size:1.0625em;"><strong><span class="glyphicon glyphicon-exclamation-sign"></span> <%= alert %></strong></p>
    </div>
  <% end %>
  <% if user_signed_in? and !current_user.registro_completo%>
    <div class="col-md-12">
      <p class="alert alert-danger" style="font-size:1.1875em;"><strong><span class="glyphicon glyphicon-exclamation-sign"></span>&nbsp;&nbsp;<%= 'Es necesario que complete su perfil para finalizar con su incripciÃ³n al congreso y poder acceder a todas las funciones del sistema' %></strong></p>
    </div>
  <%end%>
</div>
